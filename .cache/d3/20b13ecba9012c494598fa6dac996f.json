adding: 0;\r\n    list-style: none;\r\n}\r\n\r\n.list-post {\r\n    flex-basis: 30%;\r\n    margin-bottom: 2rem;\r\n    box-sizing: border-box;\r\n    background-color: white;\r\n    border-radius: 7px;\r\n    height: 100%;\r\n}\r\n\r\n.post-list-link {\r\n    text-decoration: none;\r\n    background-color: '#ffffff';\r\n    text-align: center;\r\n    text-transform: uppercase;\r\n    position: relative;\r\n}\r\n\r\n.post-list-link > span {\r\n    position: absolute;\r\n    left: 20%;\r\n} \r\n\r\n.post-list-link > img {\r\n    width: 100%;\r\n    border-radius: 7px;\r\n    height: 100%;\r\n    box-shadow: 3px 5px 8px -2px #000000;\r\n}\r\n\r\n.post-list-link> img:hover {\r\n    transform:rotate(2deg) scale(0.95) skew(-177deg) translate(1px) ;\r\n    -webkit-transform:rotate(2deg) scale(0.95) skew(-177deg) translate(1px) ;\r\n    -moz-transform:rotate(2deg) scale(0.95) skew(-177deg) translate(1px) ;\r\n    -ms-transform:rotate(2deg) scale(0.95) skew(-177deg) translate(1px) ;\r\n    -o-transform:rotate(2deg) scale(0.95) skew(-177deg) translate(1px) ;\r\n}\r\n\r\n/* Footer */\r\n.footer-comment-input {\r\n    width: 100%;\r\n    margin-bottom: 1rem;\r\n}\r\n\r\n.button {\r\n    padding: 9px;\r\n    border-radius: 8px;\r\n    border: none;\r\n}\r\n\r\n.button:hover {\r\n    cursor: pointer;\r\n}\r\n\r\n.footer-comment-button {\r\n    background-color: rgb(3, 172, 3);\r\n    color: #ffffff;\r\n}\r\n\r\n.footer-subscribe {\r\n    align-self: center;\r\n}\r\n\r\n.footer-subscribe-button {\r\n    padding-left: 3rem;\r\n    padding-right: 3rem;\r\n    background-color: rgb(211, 6, 6);\r\n    color: #ffffff;\r\n    width: 100%;\r\n    margin-top: 2rem;\r\n}\r\n\r\n@media (min-width: 600px) {\r\n    .post-list {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .list-post {\r\n        flex-basis: 40%;\r\n    }\r\n\r\n    .post-list-link > img {\r\n        height: 269px;\r\n    }\r\n\r\n    .footerContent {\r\n        display: flex;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .post-list-link > img:hover {\r\n        transform:rotate(9deg) scale(1.051) skew(-177deg) translate(1px) ;\r\n    }\r\n\r\n    .footer-subscribe-button {\r\n        margin-top: 0;\r\n    }\r\n    \r\n}\r\n\r\n@media (min-width: 700px) {\r\n    .heading {\r\n        flex-direction: row;\r\n    }\r\n    .post-list {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: space-between;\r\n    }\r\n    \r\n    .list-post {\r\n        flex-basis: 30%;\r\n    }\r\n\r\n    .post-list-link > img {\r\n        height: 350px;\r\n    }\r\n\r\n    .post-list-link > img:hover {\r\n        transform:rotate(9deg) scale(1.051) skew(-177deg) translate(1px) ;\r\n    }\r\n\r\n    .footerContent {\r\n        display: flex;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .footer-subscribe-button {\r\n        margin-top: 0;\r\n    }\r\n    \r\n    .label_menu {\r\n        display: none;\r\n    }\r\n\r\n    .list-menu {\r\n        position: unset;\r\n        box-shadow: none;\r\n        background-color: #a79890;\r\n        height: 0;\r\n        z-index: unset;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        width: unset;\r\n    }\r\n\r\n    .list-menu--item {\r\n        margin: 0;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"ea8ee9572525c32fff8f46c72ac7ef1a","cacheData":{"env":{}}}